  },
  "license": "MIT",
  "devDependencies": {
    "@types/jasmine": "^2.5.53",
    "@types/jasmine-given": "^2.6.0",
    "@types/webpack": "^2.2.16",
    "awesome-typescript-loader": "^3.1.3",
    "jasmine-given": "^2.6.4",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-given": "0.1.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-typescript": "^3.0.4",
    "karma-webpack": "^2.0.3",
    "np": "^2.16.0",
    "rimraf": "2.6.1",
    "ts-node": "^3.0.6",
    "typescript": "^2.3.4",
    "webpack": "^2.6.1"
