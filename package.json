{
  "name": "jasmine-async-spies",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/hirezio/jasmine-async-spies"
  },
  "description": "Async spies for jasmine TypeScript tests",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "scripts": {
    "clean": "rimraf src/*.js* src/*.d.ts *.d.ts",
