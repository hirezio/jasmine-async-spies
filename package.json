    "test-watch": "karma start --auto-watch --no-single-run",
    "prepublish": "npm run build",
    "postpublish": "npm run clean",
    "release": "np"
  },
  "keywords": [
    "jasmine",
    "jasmine-given",
    "spies",
    "mocks",
    "TypeScript Tests"
